.musiclist
	.up
		.search
			.select
				select(id="production")
					option(value="") 소속
					option(value="p765a") 본가
					option(value="p346") 데레
					option(value="p765m") 밀리
					option(value="p315") 사엠
					option(value="p283") 샤니
			input(id="search" type="text" placeholder="곡/아이돌/성우")
			.select
				select(id="level")
					option(value="") Lv
					option(value="1") 1
					option(value="2") 2
					option(value="3") 3
					option(value="4") 4
					option(value="5") 5
	.empty
	.bottom
		.tbl-header
			table
				thead
					tr
						th.production 소속
						th.title 곡
						th.singerCV 아이돌 / CV
						th.level Lv
		.tbl-content
			table.tablesorter
				tbody
					each song in songs
						tr(id= song)
							td.production(id =song.production)
							td.title
								.slideArea
									!=song.title
							td.singerCV
								.slideArea
									!=song.singer+'<br>'+song.CV
							td.level=song.level