include nav.pug
ul
	li= song.title
	li= song.singer
	li= song.CV
	li= song.color
	li= song.mp3
	li= song.lyric_call
	li= song.bpm
	
.contact-menu
	a(href='/db/'+ song._id + '/edit') Edit
	form(action='/db/'+ song._id +'?_method=delete' method='post')
		a(href='#' onclick="confirm('Do you want to delete this?')?this.parentElement.submit():null;") Delete
